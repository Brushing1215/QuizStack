<div id="default" class="card">
    <div class="card-body">
        <h5 class="card-title text-center">Admin page</h5>
        <hr />
        <div>
            <div class="row mx-1 ">
                <div class="col-12 col-md-10 col-lg-12 text-center">
                    <div id="highScoreTable">
                        <table class="table table-sm table-bordered text-center">
                            <thead class="thead-light">
                                <th>Question</th>
                                <th>Option</th>
                            </thead>
                            {{#each questions}}
                            <tr>
                                <td>{{questionText}}</td>
                                <td><button data-questionId="{{id}}" class="delBtn btn btn-danger">Delete!</button></td>
                            </tr>
                            {{/each}}
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script defer src="/assets/js/admin.js"></script>